{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "lib": [ "es2015", "es2017", "dom" ],
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
